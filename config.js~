CKEDITOR.editorConfig = function( config ) {
	// Define changes to default configuration here. For example:
	// config.language = 'fr';
	// config.uiColor = '#AADC6E';
   config.filebrowserBrowseUrl = '../ckeditor_mod/plugins/kcfinder/browse.php?type=files';
   config.filebrowserImageBrowseUrl = '../ckeditor_mod/plugins/kcfinder/browse.php?type=images';
   config.filebrowserFlashBrowseUrl = '../ckeditor_mod/plugins/kcfinder/browse.php?type=flash';
   config.filebrowserUploadUrl = '../ckeditor_mod/plugins/kcfinder/upload.php?type=files';
   config.filebrowserImageUploadUrl = '../ckeditor_mod/plugins/kcfinder/upload.php?type=images';
   config.filebrowserFlashUploadUrl = '../ckeditor_mod/plugins/kcfinder/upload.php?type=flash';
};
